{
  "name": "@fullstack-starter/source",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nx run-many -t build",
    "dev": "nx run-many -t serve",
    "e2e": "nx run-many -t e2e",
    "e2e:ui": "nx run-many -t e2e -- --ui",
    "format": "prettier -uw --cache .",
    "format:check": "prettier -uc --cache .",
    "lint": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint -- --fix",
    "prepare": "is-ci || husky install",
    "test": "nx run-many -t test",
    "test:coverage": "nx run-many -t test -- --coverage",
    "test:ui": "nx run-many -t test -- --ui --watch"
  },
  "lint-staged": {
    "*": [
      "prettier -uw",
      "nx affected:lint --fix true"
    ]
  },
  "dependencies": {
    "@swc/helpers": "~0.5.2",
    "axios": "^1.0.0",
    "express": "^4.18.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@nx/devkit": "17.2.8",
    "@nx/eslint": "17.2.8",
    "@nx/eslint-plugin": "17.2.8",
    "@nx/express": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/node": "17.2.8",
    "@nx/playwright": "17.2.8",
    "@nx/react": "17.2.8",
    "@nx/rspack": "17.2.2",
    "@nx/vite": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@playwright/test": "^1.36.0",
    "@rspack/core": "~0.1.12",
    "@rspack/dev-server": "~0.1.12",
    "@rspack/plugin-minify": "~0.1.12",
    "@swc-node/register": "~1.6.7",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.85",
    "@testing-library/react": "14.1.2",
    "@types/express": "4.17.21",
    "@types/node": "20.10.8",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "~1.1.3",
    "@vitest/ui": "~1.1.3",
    "eslint": "~8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-playwright": "^0.21.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unicorn": "^50.0.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jsdom": "~23.2.0",
    "lint-staged": "^15.2.0",
    "nx": "17.2.8",
    "prettier": "^3.1.1",
    "prettier-plugin-packagejson": "^2.4.9",
    "tslib": "^2.3.0",
    "typescript": "~5.3.3",
    "vite": "^5.0.0",
    "vitest": "~1.1.3"
  }
}
