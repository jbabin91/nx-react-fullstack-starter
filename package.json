{
  "name": "@fullstack-starter/source",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nx run-many -t build --parallel",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "nx run-many -t serve --parallel --projects=backend,frontend",
    "e2e": "nx run-many -t e2e --parallel",
    "e2e:ui": "nx run-many -t e2e --parallel -- --ui",
    "format": "prettier -uw --cache .",
    "format:check": "prettier -uc --cache .",
    "lint": "nx run-many -t lint --parallel",
    "lint:fix": "nx run-many -t lint --parallel -- --fix",
    "prepare": "is-ci || husky install",
    "test": "nx run-many -t test --parallel",
    "test:coverage": "nx run-many -t test --parallel -- --coverage",
    "test:ui": "nx run-many -t test --parallel -- --ui --watch"
  },
  "lint-staged": {
    "*": [
      "prettier -uw",
      "nx affected:lint -- --fix true"
    ]
  },
  "dependencies": {
